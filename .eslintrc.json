{
  "extends": [
    "google",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": false
      }
    ],
    "require-jsdoc": 0,
    "new-cap": 0,
    "no-invalid-this": 0,
    "line-break-style": 0
  },
  "parser": "babel-eslint"
}
