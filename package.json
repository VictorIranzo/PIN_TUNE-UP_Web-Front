{
  "name": "@tune-up/core",
  "version": "0.0.0",
  "description": "Tune-UP Web Angular 4 client",
  "main": "dist/tune-up.core.umd.js",
  "main:min": "dist/tune-up.core.umd.min.js",
  "jsnext:main": "src/index.js",
  "module": "dist/tune-up.core.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:nomin": "rollup -c rollup.config.nomin.js",
    "build:dev": "rollup -c rollup.config.dev.js --watch",
    "build:depl": "npm-run-all --parallel build build:nomin",
    "serve-json": "json-server --watch db.json",
    "start": "npm-run-all --parallel build:dev serve-json",
    "lint": "eslint --fix src/**/*.js",
    "depl": "npm run build:depl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starkie/TUNE-UP-Process-Web-Front.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Starkie/TUNE-UP-Process-Web-Front/issues"
  },
  "homepage": "https://github.com/Starkie/TUNE-UP-Process-Web-Front#readme",
  "dependencies": {
    "@tune-up/ng-vendor": "file:lib/ng-vendor",
    "@tune-up/primeng-vendor": "file:lib/primeng-vendor",
    "angular2-moment": "^1.7.0",
    "font-awesome": "^4.7.0",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "@tune-up/build-utils": "file:lib/build-utils",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-prettier": "^2.2.0",
    "http-server": "^0.10.0",
    "json-server": "^0.12.0",
    "npm-run-all": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^3.0.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "files": [
    "dist",
    "src"
  ]
}
